   "source": [
    "# Accept an integer number.\n",
    "# Compute and display the factorial of the number.\n",
    "num = int(input('Enter a number: '))\n",
    "\n",
    "def factorial(num):\n",
    "    if num <= 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return num * factorial(num - 1)\n",
    "\n",
    "print(f'Factorial of {num}: {factorial(num)}')"
   ]
  }
 ],
